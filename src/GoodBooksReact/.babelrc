{
    "presets": [
      "@babel/preset-env",
      "@babel/preset-react",
      "@babel/preset-env", 
      "@babel/preset-react"
    ],
    "plugins": [
      "@babel/plugin-transform-runtime",
      "@babel/plugin-transform-object-assign",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      ["@babel/plugin-proposal-class-properties", { "loose": true }]
    ]
  }