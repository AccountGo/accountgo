@model string

<div class="w-full grid grid-cols-1 gap-4">
    <div class="bg-white dark:bg-gray-800">
        <div id="icj" class="ag-fresh overflow-hidden rounded-lg shadow-md border-2 border-gray-200" style="height: 400px;"></div>
    </div>
</div>
<script>
    var columnDefs = [
    {headerName: "Id", field: "id"},
    {headerName: "Item", field: "item"},
    {headerName: "Measurement", field: "measurement"},
    {headerName: "IN", field: "in"},
    {headerName: "OUT", field: "out"},
    {headerName: "Date", field: "date"}
    ];

    var gridOptions = {
        columnDefs: columnDefs,
        rowData: @Html.Raw(Model),
        // PROPERTIES
        rowSelection: 'multiple',
    };

    // wait for the document to be loaded, otherwise
    // ag-Grid will not find the div in the document.
    document.addEventListener("DOMContentLoaded", function() {
        var eGridDiv = document.querySelector('#icj');
        new agGrid.Grid(eGridDiv, gridOptions);
    });
</script>